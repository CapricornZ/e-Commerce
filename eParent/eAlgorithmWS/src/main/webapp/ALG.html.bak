		function toggle(container){
			var parent = $(container).parent();
			var title = $(container);
			if("[-]" == title.html()){
				title.html("[+]");
				parent.find('#area').hide();
			} else {
				title.html("[-]");
				parent.find('#area').show();
			}
		}
		
