	<script type="text/javascript">
		$.ajaxSetup({
			async : false,
		});
		
		$(function(){
			
			$("div[toggle-class='switch'][positive]").jqxSwitchButton({
				height: 25,
				width: 61, 
				checked: true,
				disabled:false,
				onLabel: "正",
				offLabel:"反"
			});
			
			$("div[toggle-class='switch'][negtive]").jqxSwitchButton({
				height: 25,
				width: 61, 
				checked: false,
				disabled:true,
				onLabel: "正",
				offLabel:"反"
			});
			
			$("div[toggle-class='switch'][positive]").bind('change', function(event){
				alert($(this).val());
			});
			
			//$("div[toggle-class='switch'][positive]").hide();
			//$("div[toggle-class='switch'][negtive]").hide();
			
			$("div[toggle-class='switchExpect']").jqxSwitchButton({
				height: 25,
				width: 61, 
				checked: true,
				onLabel: "正",
				offLabel:"反"
			});
			$("div[toggle-class='switchExpect']").hide();
			
			$('#btnA').button().click(function(){
				
				var val = $('#txtDataSource').val() + 'A';
				$('#txtDataSource').val(val);
				
				var countA=0;
				var countB=0;
				var expectArr = new Array();
				processALG1($('#ALG1'), 'A', val, 'POSITIVE');
				processALG1($('#ALG1N'), 'A', val, 'NEGTIVE');
				processALG4($('#ALG4'), 'A', val);
				processNEWALG($('#NEWALG'), 'A', val, 'POSITIVE');
				processNEWALG($('#NEWALGN'), 'A', val, 'NEGTIVE');
				$("span[name='algexpect']").each(function(){
					var expect = $(this).html();
					if(null != expect){
						var array = expect.split("*");
						if('0' != array[1] && 'A' == array[0])
							countA+=parseInt(array[1]);
						if('0' != array[1] && 'B' == array[0])
							countB+=parseInt(array[1]);
					}
				});//end of EACH
				if(countA>countB)
					$('#totalExpect').html("EXPECT:A*" + (countA-countB));
				if(countB>countA)
					$('#totalExpect').html("EXPECT:B*" + (countB-countA));
				if(countB==countA)
					$('#totalExpect').html("EXPECT:A=B=" + countA);
			});
			
			$('#btnB').button().click(function(){
				
				var val = $('#txtDataSource').val() + 'B';
				$('#txtDataSource').val(val);
				
				var countA=0;
				var countB=0;
				var expectArr = new Array();
				expectArr.push(processALG1($('#ALG1'), 'B', val, 'POSITIVE'));
				expectArr.push(processALG1($('#ALG1N'), 'B', val, 'NEGTIVE'));
				expectArr.push(processALG4($('#ALG4'), 'B', val));
				expectArr.push(processNEWALG($('#NEWALG'), 'B', val, 'POSITIVE'));
				expectArr.push(processNEWALG($('#NEWALGN'), 'B', val, 'NEGTIVE'));
				$("span[name='algexpect']").each(function(){
					var expect = $(this).html();
					if(null != expect){
						var array = expect.split("*");
						if('0' != array[1] && 'A' == array[0])
							countA+=parseInt(array[1]);
						if('0' != array[1] && 'B' == array[0])
							countB+=parseInt(array[1]);
					}
				});//end of EACH
				if(countA>countB)
					$('#totalExpect').html("EXPECT:A*" + (countA-countB));
				if(countB>countA)
					$('#totalExpect').html("EXPECT:B*" + (countB-countA));
				if(countB==countA)
					$('#totalExpect').html("EXPECT:A=B=" + countA);
			});
			
			$('#btnBack').button().click(function(){
				var val = $('#txtDataSource').val();
				val = val.substring(0, val.length-1);
				$('#txtDataSource').val(val);

				var countA=0;
				var countB=0;
				var expectArr = new Array();
				expectArr.push(processALG1($('#ALG1'), 'DEL', val, 'POSITIVE'));
				expectArr.push(processALG1($('#ALG1N'), 'DEL', val, 'NEGTIVE'));
				expectArr.push(processALG4($('#ALG4'), 'DEL', val));
				expectArr.push(processNEWALG($('#NEWALG'), 'DEL', val, 'POSITIVE'));
				expectArr.push(processNEWALG($('#NEWALGN'), 'DEL', val, 'NEGTIVE'));
				$("span[name='algexpect']").each(function(){
					var expect = $(this).html();
					if(null != expect){
						var array = expect.split("*");
						if('0' != array[1] && 'A' == array[0])
							countA+=parseInt(array[1]);
						if('0' != array[1] && 'B' == array[0])
							countB+=parseInt(array[1]);
					}
				});//end of EACH
				if(countA>countB)
					$('#totalExpect').html("EXPECT:A*" + (countA-countB));
				if(countB>countA)
					$('#totalExpect').html("EXPECT:B*" + (countB-countA));
				if(countB==countA)
					$('#totalExpect').html("EXPECT:A=B=" + countA);
			});		

			$( "#createDate" ).datepicker();
			$( "#createDate" ).datepicker("option","dateFormat","yy-mm-dd");

			$('#txtDataSource').keyup(function(event){
				
				if('A' == event.key){console.log('A');}
				if('B' == event.key){console.log('B');}
				if("Backspace" == event.key){console.log('BACKSPACE');}
				return false;
			});
		});
		
		function toggle(container){
			var parent = $(container).parent();
			var title = $(container);
			if("[-]" == title.html()){
				title.html("[+]");
				parent.find('#area').hide();
			} else {
				title.html("[-]");
				parent.find('#area').show();
			}
		}
		
		function toggleAlg(container){
			var parent = $(container).parent();
			var title = $(container);
			if("[-]" == title.html()){
				title.html("[+]");
				parent.siblings('#dualArea').hide();
			} else {
				title.html("[-]");
				parent.siblings('#dualArea').show();
			}
		}
	</script>
