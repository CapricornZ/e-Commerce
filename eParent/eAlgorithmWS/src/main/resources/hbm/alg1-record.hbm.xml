<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Hibernate-mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ecommerce.algorithm1.domain.Record" table="alg1_records"> 
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="source" type="java.lang.String" column="source" length="255"/>
        <property name="trueANDfalse" type="java.lang.String" column="trueANDfalse" length="255"/>
        <property name="result" type="java.lang.String" column="result" length="255"/>
        <property name="createTime" type="java.sql.Timestamp" column="createTime"/>
    </class>
</hibernate-mapping>